        "method": "PATCH",
        "path": "/api/v1/dataset/:dataset/layer/:layer/metadata"
      }
    },
    {
      "path": "/v1/dataset/:dataset/layer/:layer/metadata",
      "method": "DELETE",
      "redirect": {
        "method": "DELETE",
        "path": "/api/v1/dataset/:dataset/layer/:layer/metadata"
      }
    },
    {
      "path": "/v1/metadata",
      "method": "GET",
      "redirect": {
