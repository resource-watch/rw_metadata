{
  "name": "Metadata service",
  "cache": [
    "metadata"
  ],
  "uncache": [
    "metadata",
    "dataset",
    "widget",
    "layer"
  ],
  "endpoints": [
    {
      "path": "/v1/dataset/:dataset/metadata",
      "method": "GET",
      "redirect": {
        "method": "GET",
        "path": "/api/v1/dataset/:dataset/metadata"
      }
    },
    {
      "path": "/v1/dataset/:dataset/metadata",
      "method": "POST",
      "redirect": {
        "method": "POST",
        "path": "/api/v1/dataset/:dataset/metadata"
      }
    },
    {
