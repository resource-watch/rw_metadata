      "method": "DELETE",
      "redirect": {
        "method": "DELETE",
        "path": "/api/v1/dataset/:dataset/widget/:widget/metadata"
      }
    },
    {
      "path": "/v1/dataset/:dataset/layer/:layer/metadata",
      "method": "GET",
